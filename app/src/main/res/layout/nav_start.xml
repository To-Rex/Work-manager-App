<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.navigation.NavigationView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="start"
    android:padding="20dp"
    android:id="@+id/navstart"
    android:background="@drawable/backgraundnav"
    tools:ignore="ExtraText">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:id="@+id/viewsettings"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/viewhot" />

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imgsozdeuser"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_margin="10dp"
            android:layout_marginTop="8dp"
            android:padding="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.CornerSize50Percent"
            app:srcCompat="@drawable/imgemale"
            app:strokeColor="@color/colorPrimaryDark"
            app:strokeWidth="0dp" />

        <ImageView
            android:id="@+id/imgkunlogo"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/imgsozdeuser"
            app:layout_constraintStart_toEndOf="@+id/imgsozdeuser"
            app:srcCompat="@drawable/astrocoinimg" />

        <TextView
            android:id="@+id/txtnavcoin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:fontFamily="sans-serif-black"
            android:gravity="fill_vertical"
            android:text="Astrocoin"
            android:textColor="@color/jigarrang"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/imgkunlogo"
            app:layout_constraintStart_toEndOf="@+id/imgkunlogo"
            app:layout_constraintTop_toTopOf="@+id/imgkunlogo" />

        <TextView
            android:id="@+id/txtnavid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginBottom="8dp"
            android:fontFamily="sans-serif-black"
            android:gravity="fill_vertical"
            android:text="id"
            android:textColor="@color/cardview_dark_background"
            android:textSize="25sp"
            app:layout_constraintBottom_toTopOf="@+id/imgkunlogo"
            app:layout_constraintStart_toStartOf="@+id/imgkunlogo"
            app:layout_constraintTop_toTopOf="@+id/imgsozdeuser"
            app:layout_constraintVertical_bias="0.071" />

        <TextView
            android:id="@+id/txtnavuser"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:layout_marginStart="16dp"
            android:fontFamily="sans-serif-black"
            android:gravity="fill_vertical"
            android:paddingLeft="5dp"
            android:paddingEnd="5dp"
            android:text="User"
            android:textSize="28sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imgsozdeuser"
            tools:ignore="RtlHardcoded" />

        <TextView
            android:id="@+id/txtnavtel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-black"
            android:gravity="fill_vertical"
            android:paddingLeft="5dp"
            android:paddingEnd="5dp"
            android:text="connect"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textColor="@color/cardview_dark_background"
            app:layout_constraintEnd_toEndOf="@+id/txtnavuser"
            app:layout_constraintStart_toStartOf="@+id/txtnavuser"
            app:layout_constraintTop_toBottomOf="@+id/txtnavuser"
            tools:ignore="RtlHardcoded,RtlSymmetry" />

        <View
            android:id="@+id/viewtop"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtnavtel" />

        <View
            android:id="@+id/viewtelegram"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/viewtop" />

        <View
            android:id="@+id/viewinstagram"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/viewtelegram" />

        <View
            android:id="@+id/viewhot"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/viewinstagram" />

        <View
            android:id="@+id/viewchiq"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/txtresetpassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="Top"
            android:textColor="@color/black"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="@+id/viewtop"
            app:layout_constraintStart_toEndOf="@+id/imgpasback"
            app:layout_constraintTop_toTopOf="@+id/viewtop" />

        <TextView
            android:id="@+id/txt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Telegram"
            android:textColor="@color/black"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="@+id/viewtelegram"
            app:layout_constraintStart_toStartOf="@+id/txtresetpassword"
            app:layout_constraintTop_toTopOf="@+id/viewtelegram" />

        <TextView
            android:id="@+id/txt2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Instagram"
            android:textColor="@color/black"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="@+id/viewinstagram"
            app:layout_constraintStart_toStartOf="@+id/txt1"
            app:layout_constraintTop_toTopOf="@+id/viewinstagram" />

        <TextView
            android:id="@+id/txt3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hot"
            android:textColor="@color/black"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="@+id/viewhot"
            app:layout_constraintStart_toStartOf="@+id/txt2"
            app:layout_constraintTop_toTopOf="@+id/viewhot" />

        <TextView
            android:id="@+id/txt4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Settings"
            android:textColor="@color/black"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="@+id/viewsettings"
            app:layout_constraintStart_toStartOf="@+id/txt3"
            app:layout_constraintTop_toTopOf="@+id/viewsettings" />

        <ImageView
            android:id="@+id/imgpasback"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:layout_constraintBottom_toBottomOf="@+id/viewtop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/viewtop"
            app:srcCompat="@drawable/imgetop" />

        <ImageView
            android:id="@+id/imguserr"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:layout_constraintBottom_toBottomOf="@+id/viewtelegram"
            app:layout_constraintStart_toStartOf="@+id/viewtelegram"
            app:layout_constraintTop_toTopOf="@+id/viewtelegram"
            app:srcCompat="@drawable/imgetelegram" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:layout_constraintBottom_toBottomOf="@+id/txt2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/viewinstagram"
            app:srcCompat="@drawable/imgeinstagram" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:layout_constraintBottom_toBottomOf="@+id/txt3"
            app:layout_constraintStart_toStartOf="@+id/viewhot"
            app:layout_constraintTop_toTopOf="@+id/txt3"
            app:srcCompat="@drawable/imgehot" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:layout_constraintBottom_toBottomOf="@+id/txt4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/txt4"
            app:srcCompat="@drawable/imgsettings" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/logout" />

        <TextView
            android:id="@+id/txt5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Log out"
            android:textColor="@color/black"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/txt4"
            app:layout_constraintTop_toTopOf="@+id/viewchiq" />
    </androidx.constraintlayout.widget.ConstraintLayout>



</com.google.android.material.navigation.NavigationView>